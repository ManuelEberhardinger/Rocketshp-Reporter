
<h3>Posts for <%= get_month(@calendar_date) %></h3>

<table class="table table-striped table-condensed">
  <tr>
    <th>Type</th>
    <th>Title</th>
    <th>Date</th>
  </tr>
  <% @posts.each do |post| %>
  <tr>
    <td><%= get_all_post_types[post.post_type].html_safe %><%= post.post_type %></td>
    <td><%= post.title %></td>
    <td><%= post.start_time.strftime("%d/%m/%Y") %></td>
  </tr>
  <% end %>
</table>
