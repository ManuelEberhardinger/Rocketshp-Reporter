<h1 id="pdf-cover">Posts</h1>

<p style='page-break-after:always;'></p>

<table class="table table-striped table-condensed">
  <tr>
    <th>Type</th>
    <th>Title</th>
    <th>Date</th>
  </tr>
  <% @posts.each do |post| %>
  <tr>
    <td><%= post.post_type %></td>
    <td><%= post.title %></td>
    <td><%= post.start_time.strftime("%d/%m/%Y") %></td>
  </tr>
  <% end %>
</table>
